<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    
    <package name="actualizarDatos" extends="default" namespace="/actualizarDatos">
        
        <action name="actualizar" class="actionActualizarDatos" method="inicio">
            <result name="ok">/servicios/actualizarDatos/formularioActualizacion.jsp</result>
            <result name="input">/servicios/actualizarDatos/formularioActualizacion.jsp</result>
        </action>
        
        <action name="actualizarDatos" class="actionActualizarDatos" method="actualizarDatos">
            <result name="input">/servicios/actualizarDatos/formularioActualizacion.jsp</result>
        </action>
        
    </package>
    
</struts>