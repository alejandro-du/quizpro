<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    
    <package name="consultarResultados" extends="default" namespace="/consultarResultados">
        
        <action name="inicio" class="actionConsultarResultados" method="inicio">
            <result name="ok">/servicios/consultarResultados/formularioResultados.jsp</result>
        </action>
        
        <action name="consultar" class="actionConsultarResultados" method="consultar">
            <result name="ok">/servicios/consultarResultados/resultados.jsp</result>
            <result name="input">/servicios/consultarResultados/formularioResultados.jsp</result>
        </action>
        
    </package>
    
</struts>
